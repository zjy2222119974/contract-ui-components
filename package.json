{
  "name": "@contract/ui-components",
  "version": "1.0.0",
  "description": "Contract共享UI组件库",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:es",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:es": "tsc -p tsconfig.build.json --module es2015 --outDir es",
    "build:watch": "npm run build:lib -- --watch",
    "clean": "rimraf lib es",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "npm run build:watch"
  },
  "keywords": [
    "taro",
    "react",
    "ui",
    "components",
    "miniprogram"
  ],
  "author": "Contract Team",
  "license": "MIT",
  "peerDependencies": {
    "@tarojs/components": "^3.6.0",
    "@tarojs/taro": "^3.6.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@tarojs/components": "^3.6.0",
    "@tarojs/taro": "^3.6.0",
    "react": "^18.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/contract-ui-components.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}